package com.yash.oops.assinment;

import com.yash.string.GarbageCollector;

public class FinalizeAssingment {
	int count=0;
	FinalizeAssingment()
	{
		++count;
		
	}
	public void finalize()
	{
		System.out.println("object are present in class"+count);
	
	
	}

	 public static void main(String args[]){
		 FinalizeAssingment s1=new FinalizeAssingment();
		 FinalizeAssingment s2=new FinalizeAssingment();
		 System.out.println("object are present in class"+count);
	  s1=null;
	  
	  System.gc();
	 }

}
